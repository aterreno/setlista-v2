{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended"
  ],
  "rules": {
    "react/no-unescaped-entities": "off",
    "@next/next/no-img-element": "off",
    "no-unused-vars": "warn",
    "no-undef": "warn",
    "no-useless-escape": "warn",
    "no-redeclare": "warn",
    "@next/next/no-html-link-for-pages": "warn",
    "react-hooks/exhaustive-deps": "warn"
  }
}
